{
  "name": "@dhaaga/mobile",
  "version": "0.14.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "expo start --dev-client -c",
    "apk": "eas build -p android --profile apk",
    "aab": "eas build -p android --profile aab",
    "client": "eas build -p android --profile dev",
    "lite:manual": "react-native build-android --mode=release && cd android && ./gradlew assembleRelease",
    "lite": "python3 ./lite.py",
    "postinstall": "cd ../.. && bun run build",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "tl:sync": "python3 ./i18n/sync.py"
