{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "checkJs": false,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "allowJs": true,
    "lib": [
      "ESNext",
      "ES2015"
    ],
    "strictNullChecks": true,
    "strict": false
  },
  "include": [
    "./*.ts",
    "./*.tsx",
    "./*.js"
  ],
  "exclude": [
    "node_modules"
  ],
  "files": ["*/**"],
  "extends": "expo/tsconfig.base"
}
