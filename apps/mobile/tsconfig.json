{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-native",
    "checkJs": false,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "allowJs": true,
    "lib": [
      "ESNext",
      "ES2015"
    ],
    "strictNullChecks": true
  },
  "include": [
    "*.ts",
    "*.tsx",
    "*.js"
  ],
  "exclude": [
    "node_modules"
  ],
  "files": [
    "./**/*"
  ],
  "extends": "expo/tsconfig.base"
}
