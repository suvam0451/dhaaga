{
	"cli": {
		"appVersionSource": "local"
	},
	"build": {
		"_shared": {
			"node": "25.2.1"
		},
		"aab": {
			"extends": "_shared",
			"android": {
				"buildType": "app-bundle"
			},
			"env": {
				"APP_VARIANT": "prod"
			}
		},
		"apk": {
			"extends": "_shared",
			"android": {
				"buildType": "apk"
			},
			"env": {
				"APP_VARIANT": "prod"
			}
		},
		"production": {
			"extends": "_shared",
			"android": {
				"buildType": "app-bundle"
			},
			"env": {
				"APP_VARIANT": "prod"
			}
		},
		"dev": {
			"extends": "_shared",
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "dev"
			}
		},
		"lite": {
			"extends": "_shared",
			"android": {
				"buildType": "apk"
			},
			"env": {
				"APP_VARIANT": "dev"
			}
		}
	}
}
