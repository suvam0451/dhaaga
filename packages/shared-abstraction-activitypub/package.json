{
  "name": "@dhaaga/shared-abstraction-activitypub",
  "description": "using design patterns to work around the differences between various Mastodon forks",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*",
      "types": "./dist/*"
    }
  },
  "version": "0.3.0",
  "dependencies": {
    "@dhaaga/shared-provider-mastodon": "*",
    "@dhaaga/shared-provider-misskey": "*",
    "axios": "^1.4.0",
    "masto": "^6.8.0",
    "megalodon": "^10.0.2",
    "misskey-js": "^2024.5.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "script": "node --loader ts-node/esm workspace.ts",
    "test": "jest --forceExit --rootDir=./test"
  },
  "type": "module"
}
