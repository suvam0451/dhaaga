{
  "name": "@dhaaga/orm",
  "description": "A custom expo-sqlite ORM implementation inspired by TypeORM",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "keywords": [
    "expo",
    "sqlite",
    "orm"
  ],
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*",
      "types": "./dist/*"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "expo-sqlite": "*"
  },
  "scripts": {
    "build": "tsgo -p ./tsconfig.json"
  }
}
